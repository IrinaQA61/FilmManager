 public class MovieManager {
 private String[] movies = new String [0]

 public void add(String movie) {
 String[] tmp = new String[movies.length +1];
 for (ing i=0; i< movies.length; i++) {
 tmp [i] = movies[i];
 }
tmp [tmp.length -1] = movie;
this.movies = tmp;
 }
 public String[] findAll () {
  return movies;
 }
